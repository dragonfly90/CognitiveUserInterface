        isa best-point
        state attend-xaxis
    =visual>
        isa text
        value =currenttime 
        
==>
    =goal>
        state read-xaxis
        current =currenttime
        
    -visual-location>
    -visual>
        !output!(=currenttime)
)

(p justify-xaxis1
    =goal>
        isa best-point
        state read-xaxis
        timestamp =currenttime
        current =currenttime
==>
    =goal>
        state read-line
        !output!(=currenttime)
    -visual-location>
    -visual>
)


(p locate-point 
    =goal>
        isa best-point
        state  read-line
        currentx	=current-x
        currenty 	=current-y    
        color1 =currentcolor
        readlabel 0
    ?visual>
        state free
==>
    +visual-location>
        isa visual-location
        screen-x =current-x
        color =currentcolor
    =goal>
        state locatePoint
        readlabel 1
)



        isa best-point
        state attendPoint
        currenty =screen-y
        readlabel 1
    =visual>
==>
    =goal>
        state  search-current-xaxis
        currenty1 =screen-y
    -visual-location>
    -visual>
)

(p read-point2
    =goal>
        isa best-point
        state attendPoint
        currenty =screen-y
        readlabel 2
    =visual>
==>
    =goal>
        state  end
        currenty2 =screen-y
    -visual-location>
    -visual>
)


(p comparision1
    =goal>
        isa best-point
        state end
        readlabel 2
        currenty1 =screen-y1
        < currenty2 =screen-y1
        labeldata2 =labeldatac
==>
    =goal>
        state  end2
        !output!(=labeldatac)
)

(p comparision2
    =goal>
        isa best-point
        state end
        readlabel 2
        currenty1 =screen-y1
